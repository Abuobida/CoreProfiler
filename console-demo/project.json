{
  "version": "1.0.0",
  "dependencies": {
    "CoreProfiler": "1.0.0",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    }
  },
  "buildOptions":{
    "emitEntryPoint": true,
    "debugType": "portable",
    "copyToOutput": [
      "coreprofiler.json"
    ]
  },
  "publishOptions": {
    "include": [
	    "coreprofiler.json"
    ]
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "portable-dnxcore50+net45+win8+wp8+wpa81"
      ]
    }
  }
}
